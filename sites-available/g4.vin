server {
    listen 80;
    listen [::]:80;
    server_name g4.vin www.g4.vin;
    return 302 https://gavin.id;
}

server {
    listen 443 ssl http2;
    listen [::]:443 ssl http2;
    include snippets/self-signed.conf;
    include snippets/ssl-params.conf;

    server_name g4.vin www.g4.vin;
    return 302 https://gavin.id;
}
